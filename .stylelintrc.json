{
  "extends": "stylelint-config-sass-guidelines",
  "rules": {
    "font-family-no-missing-generic-family-keyword": [
      true,
      {"ignoreFontFamilies": ["Genericons"]}
    ],
    "max-nesting-depth": 2,
    "selector-no-qualifying-type": null,
    "selector-max-compound-selectors": 5,
    "selector-max-id": 2
  }
}
